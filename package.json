{
    "name": "ogame-tracker",
    "version": "1.0.0",
    "description": "OGame Tracker",
    "scripts": {
        "build-typescript-files:dev:chrome": "webpack --mode=production --node-env=production --env browser=chrome dev=true",
        "build-typescript-files:dev:firefox": "webpack --mode=production --node-env=production --env browser=firefox dev=true",
        "build-typescript-files:chrome": "webpack --mode=production --node-env=production --env browser=chrome",
        "build-typescript-files:firefox": "webpack --mode=production --node-env=production --env browser=firefox",
        "dev:chrome": "npm run build-typescript-files:dev:chrome && node scripts/generate-manifest.js --dev",
        "dev:firefox": "npm run build-typescript-files:dev:firefox && node scripts/generate-manifest.js --dev",
        "build:chrome": "npm run build-typescript-files:chrome && node scripts/generate-manifest.js",
        "build:firefox": "npm run build-typescript-files:firefox && node scripts/generate-manifest.js"
    },
    "author": "Wonkydonky",
    "devDependencies": {
        "@types/chrome": "^0.0.175",
        "@types/webpack": "^5.28.0",
        "@webpack-cli/generators": "^2.4.1",
        "clean-webpack-plugin": "^4.0.0",
        "copy-webpack-plugin": "^10.2.0",
        "css-loader": "^6.5.1",
        "mini-css-extract-plugin": "^2.4.6",
        "prettier": "^2.5.1",
        "sass": "^1.47.0",
        "sass-loader": "^12.4.0",
        "ts-loader": "^9.2.6",
        "typescript": "^4.5.4",
        "webpack": "^5.65.0",
        "webpack-cli": "^4.9.1"
    },
    "dependencies": {
        "date-fns": "^2.28.0"
    }
}